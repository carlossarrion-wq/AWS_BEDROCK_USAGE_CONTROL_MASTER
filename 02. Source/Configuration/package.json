{
  "name": "aws-bedrock-usage-dashboard-tests",
  "version": "1.0.0",
  "description": "Unit tests for AWS Bedrock Usage Dashboard",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "aws",
    "bedrock",
    "dashboard",
    "testing",
    "jest"
  ],
  "author": "AWS Bedrock Team",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "tests/**/*.js",
      "!tests/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0"
  }
}
